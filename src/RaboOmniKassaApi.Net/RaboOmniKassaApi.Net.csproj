<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFrameworks>net451;netstandard2.0</TargetFrameworks>
        <TargetFramework>net451</TargetFramework>
        <Version>2.0.5</Version>
        <Copyright>Copyright Rob Janssen 2019</Copyright>
        <Description>A C#/.net wrapper for the RaboOmniKassa 2.0 API</Description>
        <PackageProjectUrl>https://github.com/janssenr/RaboOmniKassaApi.Net</PackageProjectUrl>
        <PackageIconUrl>https://www.rabobank.nl/static/generic/css/images/favicon.ico</PackageIconUrl>
        <PackageTags>raboomnikassa rabo omnikassa 2.0 api rest data</PackageTags>
        <PackageReleaseNotes>Convert to .Net Standard</PackageReleaseNotes>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <Authors>Rob Janssen</Authors>
        <Company />
        <PackageId>RaboOmniKassaApi</PackageId>
        <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    </PropertyGroup>
    <ItemGroup Condition="'$(TargetFramework)' == 'net451'">
        <Reference Include="System.Net.Http" />
	</ItemGroup>
</Project>